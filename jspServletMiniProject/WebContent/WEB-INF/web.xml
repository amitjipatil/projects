<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name>jspServletMiniProject</display-name>
	<servlet>
	<servlet-name>abc</servlet-name>
	<servlet-class>in.hybridsoft.facebook.servlet.LoginServlet</servlet-class>
	</servlet>
    <servlet-mapping>
    <servlet-name>abc</servlet-name>
    <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
    <servlet>
    <servlet-name>reg</servlet-name>
	<servlet-class>in.hybridsoft.facebook.servlet.RegisterServlet</servlet-class>
	</servlet> 
    <servlet-mapping>
    <servlet-name>reg</servlet-name>
    <url-pattern>/register</url-pattern>
    </servlet-mapping>
    
    <servlet>
    <servlet-name>lout</servlet-name>
	<servlet-class>in.hybridsoft.facebook.servlet.LogoutServlet</servlet-class>
	</servlet> 
    <servlet-mapping>
    <servlet-name>lout</servlet-name>
    <url-pattern>/logout</url-pattern>
    </servlet-mapping> 
    
    <servlet>
    <servlet-name>addinfo</servlet-name>
	<servlet-class>in.hybridsoft.facebook.servlet.ProfileServlet</servlet-class>
	</servlet> 
    <servlet-mapping>
    <servlet-name>addinfo</servlet-name>
    <url-pattern>/addinfo</url-pattern>
    </servlet-mapping>
    
     <servlet>
    <servlet-name>changepwd</servlet-name>
	<servlet-class>in.hybridsoft.facebook.servlet.ChangePasswordServlet</servlet-class>
	</servlet> 
    <servlet-mapping>
    <servlet-name>changepwd</servlet-name>
    <url-pattern>/changepass</url-pattern>  
    </servlet-mapping> 
    
    <servlet>
    <servlet-name>del</servlet-name>
	<servlet-class>in.hybridsoft.facebook.servlet.DeleteEmailServlet</servlet-class>
	</servlet> 
    <servlet-mapping>
    <servlet-name>del</servlet-name>
    <url-pattern>/deleteemail</url-pattern>
    </servlet-mapping>
    
     
     <servlet>
    <servlet-name>mms</servlet-name>
	<servlet-class>in.hybridsoft.facebook.servlet.AddNewEmailServlet</servlet-class>
	</servlet> 
	
    <servlet-mapping>
    <servlet-name>mms</servlet-name>
    <url-pattern>/savemail</url-pattern>
    </servlet-mapping>
    
    
	<welcome-file-list>
		<welcome-file>jsp/index.jsp</welcome-file>
	</welcome-file-list>
</web-app>