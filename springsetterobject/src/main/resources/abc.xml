<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
"http://www.springframework.org/dtd/spring-beans-2.0.dtd">
 
<beans>

<bean id="id2" class="mypack.Travel">

<property name="v" ref="id1"></property>

</bean>

<bean id="id1" class="mypack.Car">
<property name="fueltype" value="petrol" />
<property name="highspeed" value="200" />
</bean>

<bean id="id3" class="mypack.Bus">
<property name="fueltype" value="disel" />
<property name="speed" value="90" />
</bean>

</beans>
